{
	"name": "@collecthor/file-browser",
	"private": false,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"package": "svelte-kit sync && svelte-package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"eslint": "eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:playwright": "npx playwright test --trace on",
		"test:vitest": "vitest watch --ui --open",
		"test:vitest:ci": "vitest run",
		"generate": "npx openapi-typescript ./reference/file-browser-api.yaml -o ./src/lib/interfaces/api.generated.d.ts"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1.15.9",
		"svelte": "^3.58"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/collecthor/collecthor-file-browser"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.3",
		"@commitlint/config-conventional": "^17.6.3",
		"@openapitools/openapi-generator-cli": "^2.6.0",
		"@playwright/test": "^1.34.3",
		"@stoplight/prism-cli": "^5",
		"@sveltejs/kit": "^1",
		"@sveltejs/package": "^2.0.2",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^4",
		"@tsconfig/svelte": "^5",
		"@types/testing-library__jest-dom": "^5.14.6",
		"@typescript-eslint/eslint-plugin": "^6",
		"@typescript-eslint/parser": "^6",
		"@vitest/coverage-v8": "^0.33.0",
		"@vitest/ui": "^0.33.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-svelte": "^2",
		"husky": "^8.0.3",
		"jsdom": "^22.0.0",
		"json-schema-to-typescript": "^13.0.1",
		"openapi-typescript": "^6.2.6",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.1",
		"svelte": "^4",
		"svelte-check": "^3",
		"svelte-preprocess": "^5",
		"tslib": "^2.3.1",
		"typescript": "^5",
		"vite": "^4",
		"vitest": "^0.33"
	},
	"dependencies": {
		"@collecthor/svelte-material-icons": "^1.3",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/adapter-static": "^2.0.2",
		"eventemitter3": "^5.0.1",
		"openapi-fetch": "^0.6.1",
		"strongly-typed-events": "^3.0.9",
		"svelte-simple-modal": "^1.6"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"publishConfig": {
		"provenance": true,
		"access": "public"
	}
}
