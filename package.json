{
  "name": "collecthor-file-browser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "mock-server": "prism mock ./reference/file-browser-api.yaml --dynamic",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^4.10.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.1.3",
    "@tsconfig/svelte": "^2.0.1",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "ts-jest": "^28.0.8",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "svelte-material-icons": "^2.0.4",
    "svelte-simple-modal": "^1.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.svelte$": ["svelte-jester", {
        "preprocess": true
      }]
    },
    "moduleFileExtensions": [
      "js",
      "svelte",
      "ts"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
