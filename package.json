{
	"name": "@collecthor/file-browser",
	"private": false,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"package": "svelte-kit sync && svelte-package && vite -c vite.browser.js build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"eslint": "eslint .",
		"format": "prettier --write .",
		"test:playwright": "npx playwright test --trace on",
		"test:vitest": "vitest watch --ui --open",
		"test:vitest:ci": "vitest run",
		"generate": "npx openapi-typescript ./reference/file-browser-api.yaml -o ./src/lib/interfaces/api.generated.d.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/collecthor/collecthor-file-browser.git"
	},
	"devDependencies": {
		"@commitlint/cli": "^19",
		"@commitlint/config-conventional": "^19",
		"@eslint/js": "^9.10.0",
		"@iconify-json/mdi": "^1.2.0",
		"@openapitools/openapi-generator-cli": "^2.6.0",
		"@playwright/test": "^1.34.3",
		"@stoplight/prism-cli": "^5",
		"@sveltejs/adapter-auto": "^3",
		"@sveltejs/adapter-static": "^3",
		"@sveltejs/kit": "^2",
		"@sveltejs/package": "^2.3",
		"@testing-library/jest-dom": "^6",
		"@testing-library/svelte": "^5",
		"@tsconfig/svelte": "^5",
		"@types/jest": "^29.5.12",
		"@types/winbox": "^0.2.1",
		"@vitest/coverage-v8": "^2",
		"@vitest/ui": "^2",
		"eslint": "^9",
		"eslint-plugin-promise": "^7",
		"eslint-plugin-svelte": "^2",
		"globals": "^15.12.0",
		"husky": "^9",
		"jsdom": "^24",
		"json-schema-to-typescript": "^15.0.2",
		"openapi-typescript": "^7",
		"prettier": "^3",
		"prettier-plugin-svelte": "^3",
		"sass": "^1.62.1",
		"svelte-check": "^4",
		"svelte-preprocess": "^6",
		"tslib": "^2.3.1",
		"typescript": "^5",
		"typescript-eslint": "^8.1.0",
		"unplugin-icons": "^0.20",
		"vite": "^5",
		"vitest": "^2"
	},
	"dependencies": {
		"eventemitter3": "^5.0.1",
		"monaco-editor": "^0.50",
		"openapi-fetch": "^0.12",
		"rxjs": "^7.8.1",
		"strongly-typed-events": "^3.0.9",
		"svelte": "^4.2",
		"winbox": "github:collecthor/winbox#fix-support-ssr"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"browser",
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"publishConfig": {
		"provenance": true,
		"access": "public",
		"registry": "https://npm.pkg.github.com"
	}
}
